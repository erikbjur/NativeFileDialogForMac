{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Windows FileDialogsApp",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/ARM64/Debug/FileDialogApp/FileDialogApp.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "console": "integratedTerminal"
    },
    {
      "name": "Debug MacOSX FileDialogsApp",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/arm64/Debug/FileDialogApp/FileDialogApp.app/Contents/MacOS/FileDialogApp",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for LLDB",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "osx": {
        "MIMode": "lldb"
      },
      "logging": {
        "moduleLoad": false,
        "trace": false
      }
    }
  ]
}